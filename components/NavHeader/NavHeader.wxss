/* components/NavHeader/NavHeader.wxss */
.header {
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
}
.header .title{
  font-size: 26rpx;
  line-height: 60rpx;
  color: #666;
}
.header view {
  display: flex;
  justify-content: space-between;
}
.header .more {
  border: 1px solid #333;
  border-radius: 30rpx;
  font-size: 24rpx;
  padding: 10rpx 20rpx;
}